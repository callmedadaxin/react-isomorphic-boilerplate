{
  "presets": [
    [
      "env"
    ],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "add-module-exports",
    "syntax-dynamic-import",
    "dynamic-import-node",
    "react-hot-loader/babel",
    "react-loadable/babel",
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ]
  ]
}